{
	"name": "hocon-helper",
	"displayName": "hocon-helper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.copyHoconKey"
	],
	"main": "./src/main.js",
	"contributes": {
		"commands": [{
            "command": "extension.copyHoconKey",
			"title": "Copy Hocon Key"
		}],
		"menus": {
			"editor/context": [{
				"when": "editorLangId == properties",
				"command": "extension.copyHoconKey",
				"group": "9_cutcopypaste@10"
			}]
		}
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	}
}
